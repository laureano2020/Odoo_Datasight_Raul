                <xpath expr="//form/sheet/notebook/page[@name='invoice_tab']/field[@name='invoice_line_ids']/tree/field[@name='sequence']" position="after">
                    <field name="print"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>