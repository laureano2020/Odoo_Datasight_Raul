<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="alia_account_move_form_inherit_print_inherit" model="ir.ui.view">
            <field name="name">alia.account.move.form.inherit.print.inherit</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
